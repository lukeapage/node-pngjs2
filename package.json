{
  "name": "pngjs",
  "version": "3.3.1",
  "description": "PNG encoder/decoder in pure JS, supporting any bit size & interlace, async & sync with full test suite.",
  "contributors": [
    "Alexandre ParÃ©",
    "Gaurav Mali",
    "Gusts Kaksis",
    "Kuba Niegowski",
    "Luke Page",
    "Pietajan De Potter",
    "Steven Sojka",
    "liangzeng",
    "Michael Vogt",
    "Xin-Xin Wang",
    "toriningen",
    "Eugene Kulabuhov"
  ],
  "homepage": "https://github.com/lukeapage/pngjs",
  "keywords": [
    "PNG",
    "decoder",
    "encoder",
    "js-png",
    "node-png",
    "parser",
    "png",
    "png-js",
    "png-parse",
    "pngjs"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "./dist/png.js",
  "directories": {
    "lib": "dist",
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "build": "npm run compile && npm run prepublish",
    "prepublish": "npm run browserify",
    "clean:dist": "rimraf dist",
    "compile": "npm run clean:dist && babel -d dist/ lib/",
    "browserify": "browserify lib/png.js --standalone png > browser.js",
    "coverage": "istanbul -- cover node_modules/tape/bin/tape test/*-spec.js nolarge",
    "coverage-report": "npm run coverage && istanbul report html",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test": "npm run lint && babel-tape-runner test/*-spec.js | tap-dot && node test/run-compare",
    "test:watch": "nodemon --exec 'npm run test' -e js,json",
    "lint": "eslint lib",
    "lint:fix": "eslint lib --fix"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/lukeapage/pngjs2.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukeapage/pngjs2/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-tape-runner": "^2.0.1",
    "browserify": "^14.5.0",
    "buffer-equal": "1.0.0",
    "connect": "^3.6.5",
    "eslint": "^4.13.1",
    "istanbul": "^0.4.4",
    "nodemon": "^1.13.3",
    "phantomjs-prebuilt": "^2.1.16",
    "rimraf": "^2.6.2",
    "serve-static": "^1.13.1",
    "tap-dot": "^1.0.0",
    "tape": "^4.8.0"
  }
}
